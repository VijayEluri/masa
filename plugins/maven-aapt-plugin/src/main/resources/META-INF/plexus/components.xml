<component-set>
	<!-- com.pyx4me:proguard-maven-plugin:proguard, -->
	<components>
		<component>
			<role>org.apache.maven.lifecycle.mapping.LifecycleMapping</role>
			<role-hint>android:apk</role-hint>
			<implementation>
				org.apache.maven.lifecycle.mapping.DefaultLifecycleMapping
			</implementation>
			<configuration>
				<phases>
					<validate>
						org.jvending.masa.plugins:maven-toolchains-plugin:toolchain
					</validate>
					<generate-sources>
						org.jvending.masa.plugins:maven-reslinks-plugin:link,
						org.jvending.masa.plugins:maven-aidl-plugin:generate
					</generate-sources>
					<process-resources>
						org.jvending.masa.plugins:maven-localization-plugin:copy,
						org.jvending.masa.plugins:maven-aapt-plugin:unpack,
						org.apache.maven.plugins:maven-resources-plugin:resources
					</process-resources>
					<compile>
						org.jvending.masa.plugins:maven-aapt-plugin:compile,
						org.apache.maven.plugins:maven-compiler-plugin:compile
					</compile>
					<process-classes>
						org.apache.maven.plugins:maven-jar-plugin:jar,
						org.jvending.masa.plugins:maven-proguard-plugin:proguard,
						org.jvending.masa.plugins:maven-dx-plugin:dx
					</process-classes>
					<process-test-resources>
						org.apache.maven.plugins:maven-resources-plugin:testResources
					</process-test-resources>
					<test-compile>org.apache.maven.plugins:maven-compiler-plugin:testCompile
					</test-compile>
					<test>org.apache.maven.plugins:maven-surefire-plugin:test</test>
					<package>
						org.jvending.masa.plugins:maven-aapt-plugin:package,
						org.jvending.masa.plugins:maven-apkbuilder-plugin:build,
						org.jvending.masa.plugins:maven-zipalign-plugin:align
					</package>
					<integration-test>org.jvending.masa.plugins:maven-adb-plugin:install
					</integration-test>
					<verify>org.jvending.masa.plugins:maven-lint-plugin:verify</verify>
					<install>
						org.apache.maven.plugins:maven-install-plugin:install
					</install>
					<deploy>org.apache.maven.plugins:maven-deploy-plugin:deploy
					</deploy>
				</phases>
			</configuration>
		</component>
		<component>
			<role>org.apache.maven.lifecycle.mapping.LifecycleMapping</role>
			<role-hint>android:lib</role-hint>
			<implementation>
				org.apache.maven.lifecycle.mapping.DefaultLifecycleMapping
			</implementation>
			<configuration>
				<phases>
					<validate>
						org.jvending.masa.plugins:maven-toolchains-plugin:toolchain
					</validate>
					<generate-sources>
						org.jvending.masa.plugins:maven-aidl-plugin:generate
					</generate-sources>
					<process-resources>
						org.apache.maven.plugins:maven-resources-plugin:resources
					</process-resources>
					<compile>
						org.jvending.masa.plugins:maven-aapt-plugin:compile,
						org.apache.maven.plugins:maven-compiler-plugin:compile
					</compile>
					<process-classes>
						org.apache.maven.plugins:maven-jar-plugin:jar
					</process-classes>
					<package>
						org.jvending.masa.plugins:maven-aapt-plugin:lib-package
					</package>
					<install>
						org.apache.maven.plugins:maven-install-plugin:install
					</install>
					<deploy>org.apache.maven.plugins:maven-deploy-plugin:deploy
					</deploy>
				</phases>
			</configuration>
		</component>
		<component>
			<role>org.apache.maven.lifecycle.mapping.LifecycleMapping</role>
			<role-hint>android:apk:platformTest</role-hint>
			<implementation>
				org.apache.maven.lifecycle.mapping.DefaultLifecycleMapping
			</implementation>
			<configuration>
				<phases>
					<process-resources>org.apache.maven.plugins:maven-resources-plugin:resources
					</process-resources>
					<compile>
						org.jvending.masa.plugins:maven-aapt-plugin:compile,
						org.apache.maven.plugins:maven-compiler-plugin:compile
					</compile>
					<process-classes>
						org.apache.maven.plugins:maven-jar-plugin:jar,
						org.jvending.masa.plugins:maven-dx-plugin:dx
					</process-classes>
					<test-compile>org.apache.maven.plugins:maven-compiler-plugin:testCompile
					</test-compile>
					<package>
						org.jvending.masa.plugins:maven-aapt-plugin:package,
						org.jvending.masa.plugins:maven-apkbuilder-plugin:build
					</package>
					<integration-test>
						org.jvending.masa.plugins:maven-adb-plugin:install,
						org.jvending.masa.plugins:maven-platformtest-plugin:test
					</integration-test>
				</phases>
			</configuration>
		</component>
		<component>
			<role>org.apache.maven.artifact.handler.ArtifactHandler</role>
			<role-hint>android:dex</role-hint>
			<implementation>org.apache.maven.artifact.handler.DefaultArtifactHandler
			</implementation>
			<configuration>
				<extension>dex</extension>
				<type>android:dex</type>
			</configuration>
		</component>
		<component>
			<role>org.apache.maven.artifact.handler.ArtifactHandler</role>
			<role-hint>android:apk</role-hint>
			<implementation>org.apache.maven.artifact.handler.DefaultArtifactHandler
			</implementation>
			<configuration>
				<extension>apk</extension>
				<type>android:apk</type>
			</configuration>
		</component>
		<component>
			<role>org.apache.maven.artifact.handler.ArtifactHandler</role>
			<role-hint>android:lib</role-hint>
			<implementation>org.apache.maven.artifact.handler.DefaultArtifactHandler
			</implementation>
			<configuration>
				<extension>jar</extension>
				<type>android:lib</type>
			</configuration>
		</component>
	</components>
</component-set>